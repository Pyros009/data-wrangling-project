   "source": [
    "# Normalization 1 (average gains) - Assuming 'Total' is the column you want to apply the formula to\n",
    "out5['Value'] = (out5['Total'] - out5['Total'].shift(-1)) / out5['Total']\n",
    "display(out5)\n",
    "#display(txt2)"
   ]
